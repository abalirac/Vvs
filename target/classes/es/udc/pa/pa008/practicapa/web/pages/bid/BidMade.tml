	<h4>${message:title}</h4>

	<p>${message:bid-label}</p>

</div>

<p>
	<a href="#" t:type="PageLink" t:page="product/productdetails" t:context="productId">${message:link-product}</a>
</p>

<p>
	<a href="#" t:type="PageLink" t:page="index">${message:link-home}</a>
</p>

</html>